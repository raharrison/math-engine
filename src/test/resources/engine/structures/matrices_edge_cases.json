{
    "category": "Matrix Edge Cases",
    "tests": [
        {
            "id": "mat_edge_001",
            "input": "[[1]]",
            "expected": "[[1]]",
            "expectedType": "NodeMatrix",
            "notes": "1x1 matrix"
        },
        {
            "id": "mat_edge_002",
            "input": "[[1]][0][0]",
            "expected": 1.0,
            "expectedType": "NodeRational",
            "notes": "Access element of 1x1 matrix"
        },
        {
            "id": "mat_edge_003",
            "input": "det([[5]])",
            "expected": 5.0,
            "expectedType": "NodeDouble",
            "notes": "Determinant of 1x1 matrix"
        },
        {
            "id": "mat_edge_004",
            "input": "[[1, 2]]",
            "expected": "[[1, 2]]",
            "expectedType": "NodeMatrix",
            "notes": "1x2 matrix (row vector)"
        },
        {
            "id": "mat_edge_005",
            "input": "[[1], [2]]",
            "expected": "[[1], [2]]",
            "expectedType": "NodeMatrix",
            "notes": "2x1 matrix (column vector)"
        },
        {
            "id": "mat_edge_006",
            "input": "[[1, 0], [0, 1]]",
            "expected": "[[1, 0], [0, 1]]",
            "expectedType": "NodeMatrix",
            "notes": "2x2 identity matrix"
        },
        {
            "id": "mat_edge_007",
            "input": "det([[1, 0], [0, 1]])",
            "expected": 1.0,
            "expectedType": "NodeDouble",
            "notes": "Determinant of identity matrix"
        },
        {
            "id": "mat_edge_008",
            "input": "det([[2, 0], [0, 3]])",
            "expected": 6.0,
            "expectedType": "NodeDouble",
            "notes": "Determinant of diagonal matrix"
        },
        {
            "id": "mat_edge_009",
            "input": "[[1, 2, 3], [4, 5, 6]]",
            "expected": "[[1, 2, 3], [4, 5, 6]]",
            "expectedType": "NodeMatrix",
            "notes": "2x3 matrix"
        },
        {
            "id": "mat_edge_010",
            "input": "[[1, 2], [3, 4], [5, 6]]",
            "expected": "[[1, 2], [3, 4], [5, 6]]",
            "expectedType": "NodeMatrix",
            "notes": "3x2 matrix"
        },
        {
            "id": "mat_edge_011",
            "input": "[[1, 2], [3, 4]] + [[5, 6], [7, 8]]",
            "expected": "[[6, 8], [10, 12]]",
            "expectedType": "NodeMatrix",
            "notes": "Matrix addition"
        },
        {
            "id": "mat_edge_012",
            "input": "[[10, 20], [30, 40]] - [[1, 2], [3, 4]]",
            "expected": "[[9, 18], [27, 36]]",
            "expectedType": "NodeMatrix",
            "notes": "Matrix subtraction"
        },
        {
            "id": "mat_edge_013",
            "input": "[[1, 2], [3, 4]] * 2",
            "expected": "[[2, 4], [6, 8]]",
            "expectedType": "NodeMatrix",
            "notes": "Scalar multiplication of matrix"
        },
        {
            "id": "mat_edge_014",
            "input": "3 * [[1, 2], [3, 4]]",
            "expected": "[[3, 6], [9, 12]]",
            "expectedType": "NodeMatrix",
            "notes": "Scalar multiplication (reversed)"
        },
        {
            "id": "mat_edge_015",
            "input": "-[[1, 2], [3, 4]]",
            "expected": "[[-1, -2], [-3, -4]]",
            "expectedType": "NodeMatrix",
            "notes": "Matrix negation"
        },
        {
            "id": "mat_edge_016",
            "input": "[[1, 2], [3, 4]] @ [[5, 6], [7, 8]]",
            "expected": "[[19, 22], [43, 50]]",
            "expectedType": "NodeMatrix",
            "notes": "Matrix multiplication 2x2 * 2x2"
        },
        {
            "id": "mat_edge_017",
            "input": "[[1, 2, 3], [4, 5, 6]] @ [[1], [2], [3]]",
            "expected": "[[14], [32]]",
            "expectedType": "NodeMatrix",
            "notes": "Matrix multiplication 2x3 * 3x1"
        },
        {
            "id": "mat_edge_018",
            "input": "transpose([[1, 2], [3, 4]])",
            "expected": "[[1, 3], [2, 4]]",
            "expectedType": "NodeMatrix",
            "notes": "Transpose 2x2 matrix"
        },
        {
            "id": "mat_edge_019",
            "input": "transpose([[1, 2, 3]])",
            "expected": "[[1], [2], [3]]",
            "expectedType": "NodeMatrix",
            "notes": "Transpose row to column"
        },
        {
            "id": "mat_edge_020",
            "input": "transpose([[1], [2], [3]])",
            "expected": "[[1, 2, 3]]",
            "expectedType": "NodeMatrix",
            "notes": "Transpose column to row"
        },
        {
            "id": "mat_edge_021",
            "input": "trace([[1, 2], [3, 4]])",
            "expected": 5.0,
            "expectedType": "NodeDouble",
            "notes": "Trace of 2x2 matrix"
        },
        {
            "id": "mat_edge_022",
            "input": "trace([[1, 2, 3], [4, 5, 6], [7, 8, 9]])",
            "expected": 15.0,
            "expectedType": "NodeDouble",
            "notes": "Trace of 3x3 matrix"
        },
        {
            "id": "mat_edge_023",
            "input": "det([[1, 2, 3], [4, 5, 6], [7, 8, 9]])",
            "expected": 0.0,
            "expectedType": "NodeDouble",
            "notes": "Determinant of singular matrix"
        },
        {
            "id": "mat_edge_024",
            "input": "[[1, 2], [3, 4]][0]",
            "expected": "{1, 2}",
            "expectedType": "NodeVector",
            "notes": "Access row of matrix"
        },
        {
            "id": "mat_edge_025",
            "input": "[[1, 2], [3, 4]][1][1]",
            "expected": 4.0,
            "expectedType": "NodeRational",
            "notes": "Access specific element"
        }
    ]
}

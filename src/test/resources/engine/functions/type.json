        },
        {
            "id": "int_002",
            "input": "int(-5.7)",
            "expected": -5,
            "expectedType": "NodeRational",
            "notes": "Truncate negative toward zero"
        },
        {
            "id": "int_003",
            "input": "int(5)",
            "expected": 5,

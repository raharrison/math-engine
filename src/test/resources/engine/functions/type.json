{
    "category": "Type Functions",
    "description": "Comprehensive tests for isnan, isinf, isfinite, isint, iseven, isodd, ispositive, isnegative, iszero, int, float, bool, numerator, denominator, typeof, isnumber, isvector, ismatrix, isboolean",
    "tests": [
        {
            "id": "isnan_001",
            "input": "isnan(0/0)",
            "expected": true,
            "expectedType": "NodeBoolean",
            "notes": "0/0 produces NaN"
        },
        {
            "id": "isnan_002",
            "input": "isnan(5)",
            "expected": false,
            "expectedType": "NodeBoolean",
            "notes": "Regular number is not NaN"
        },
        {
            "id": "isnan_003",
            "input": "isnan(1/0)",
            "expected": false,
            "expectedType": "NodeBoolean",
            "notes": "Infinity is not NaN"
        },
        {
            "id": "isinf_001",
            "input": "isinf(1/0)",
            "expected": true,
            "expectedType": "NodeBoolean",
            "notes": "1/0 produces infinity"
        },
        {
            "id": "isinf_002",
            "input": "isinf(-1/0)",
            "expected": true,
            "expectedType": "NodeBoolean",
            "notes": "Negative infinity"
        },
        {
            "id": "isinf_003",
            "input": "isinf(5)",
            "expected": false,
            "expectedType": "NodeBoolean",
            "notes": "Regular number is finite"
        },
        {

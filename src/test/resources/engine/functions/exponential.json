        {
            "id": "pow_005",
            "input": "pow(4, 0.5)",
            "expected": 2.0,
            "expectedType": "NodeDouble",
            "notes": "4^0.5 = sqrt(4) = 2"
        },
        {
            "id": "identity_001",
            "input": "ln(exp(5))",
            "expected": 5.0,
            "expectedType": "NodeDouble",
            "notes": "ln(e^5) = 5"
        },
        {
            "id": "identity_002",
            "input": "exp(ln(5))",
            "expected": 5.0,
            "expectedType": "NodeDouble",
            "notes": "e^ln(5) = 5"
        },
        {
            "id": "identity_003",
            "input": "sqrt(16) + cbrt(27)",

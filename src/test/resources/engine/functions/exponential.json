            "notes": "4th root of 16 = 2"
        },
        {
            "id": "nroot_002",
            "input": "nroot(32, 5)",
            "expected": 2.0,
            "expectedType": "NodeDouble",
            "notes": "5th root of 32 = 2"
        },
        {
            "id": "pow_001",
            "input": "pow(2, 3)",
            "expected": 8.0,
            "expectedType": "NodeDouble",
            "notes": "2^3 = 8"
        },
        {
            "id": "pow_002",
            "input": "pow(10, 2)",
            "expected": 100.0,
            "expectedType": "NodeDouble",
            "notes": "10^2 = 100"
        },
        {
            "id": "pow_003",
            "input": "pow(5, 0)",
            "expected": 1.0,
            "expectedType": "NodeDouble",
            "notes": "5^0 = 1"
        },
        {
            "id": "pow_004",
            "input": "pow(2, -1)",
            "expected": 0.5,
            "expectedType": "NodeDouble",
            "notes": "2^-1 = 0.5"

{
    "category": "Exponential and Logarithmic Functions",
    "description": "Comprehensive tests for exp, exp2, exp10, expm1, ln, log, log2, logn, log1p, sqrt, cbrt, nroot, pow",
    "tests": [
        {
            "id": "exp_001",
            "input": "exp(0)",
            "expected": 1.0,
            "expectedType": "NodeDouble",
            "notes": "e^0 = 1"
        },
        {
            "id": "exp_002",
            "input": "exp(1)",
            "expected": 2.718281828459045,
            "expectedType": "NodeDouble",
            "notes": "e^1 = e"
        },
        {
            "id": "exp_003",
            "input": "exp(-1)",
            "expected": 0.36787944117144233,
            "expectedType": "NodeDouble",
            "notes": "e^-1 = 1/e"
        },
        {
            "id": "exp_004",
            "input": "exp(2)",
            "expected": 7.38905609893065,
            "expectedType": "NodeDouble",
            "notes": "e^2"
        },
        {
            "id": "exp2_001",
            "input": "exp2(0)",
            "expected": 1.0,
            "expectedType": "NodeDouble",
            "notes": "2^0 = 1"
        },
        {

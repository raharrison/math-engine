{
    "category": "Subscript and Slice Operations",
    "tests": [
        {
            "id": "subscript_001",
            "input": "{10, 20, 30}[0]",
            "expected": 10,
            "expectedType": "NodeRational",
            "notes": "First element (0-indexed)"
        },
        {
            "id": "subscript_002",
            "input": "{10, 20, 30}[1]",
            "expected": 20,
            "expectedType": "NodeRational",
            "notes": "Second element"
        },
        {
            "id": "subscript_003",
            "input": "{10, 20, 30}[2]",
            "expected": 30,
            "expectedType": "NodeRational",
            "notes": "Third element"
        },
        {
            "id": "subscript_004",
            "input": "{10, 20, 30, 40, 50}[-1]",
            "expected": 50,
            "expectedType": "NodeRational",
            "notes": "Last element (negative index)"
        },
        {
            "id": "subscript_005",
            "input": "{10, 20, 30, 40, 50}[-2]",
            "expected": 40,
            "expectedType": "NodeRational",
            "notes": "Second from last"
        },
        {
            "id": "subscript_006",
            "input": "{10, 20, 30, 40, 50}[1:3]",
            "expected": [
                20,
                30
            ],
            "expectedType": "NodeVector",
            "notes": "Slice (inclusive:exclusive)"
        },
        {
            "id": "subscript_007",
            "input": "{10, 20, 30, 40, 50}[:3]",
            "expected": [
                10,
                20,
                30
            ],
            "expectedType": "NodeVector",
            "notes": "Slice from start"
        },
        {
            "id": "subscript_008",
            "input": "{10, 20, 30, 40, 50}[2:]",
            "expected": [
                30,
                40,
                50
            ],
            "expectedType": "NodeVector",
            "notes": "Slice to end"
        },
        {
            "id": "subscript_009",
            "input": "{10, 20, 30, 40, 50}[:]",
            "expected": [
                10,
                20,
                30,
                40,
                50
            ],
            "expectedType": "NodeVector",
            "notes": "Full copy via slice"
        },
        {
            "id": "subscript_010",
            "input": "{10, 20, 30, 40, 50}[1:4]",
            "expected": [
                20,
                30,
                40
            ],
            "expectedType": "NodeVector",
            "notes": "Slice middle portion"
        },
        {
            "id": "subscript_011",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][0, 0]",
            "expected": 1,
            "expectedType": "NodeRational",
            "notes": "Matrix element (0,0)"
        },
        {
            "id": "subscript_012",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][1, 2]",
            "expected": 6,
            "expectedType": "NodeRational",
            "notes": "Matrix element (1,2)"
        },
        {
            "id": "subscript_013",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][2, 1]",
            "expected": 8,
            "expectedType": "NodeRational",
            "notes": "Matrix element (2,1)"
        },
        {
            "id": "subscript_014",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][0, :]",
            "expected": [
                1,
                2,
                3
            ],
            "expectedType": "NodeVector",
            "notes": "Matrix row 0"
        },
        {
            "id": "subscript_015",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][:, 1]",
            "expected": [
                2,
                5,
                8
            ],
            "expectedType": "NodeVector",
            "notes": "Matrix column 1"
        },
        {
            "id": "subscript_016",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][0:2, 1:3]",
            "expected": "[[2, 3], [5, 6]]",
            "expectedType": "NodeMatrix",
            "notes": "Sub-matrix slice"
        },
        {
            "id": "subscript_017",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][1:, :]",
            "expected": "[[4, 5, 6], [7, 8, 9]]",
            "expectedType": "NodeMatrix",
            "notes": "Bottom two rows"
        },
        {
            "id": "subscript_018",
            "input": "[1, 2, 3; 4, 5, 6; 7, 8, 9][:, :2]",
            "expected": "[[1, 2], [4, 5], [7, 8]]",
            "expectedType": "NodeMatrix",
            "notes": "First two columns"
        },
        {
            "id": "subscript_019",
            "input": "{10, 20, 30}[0] + {10, 20, 30}[1]",
            "expected": 30,
            "expectedType": "NodeRational",
            "notes": "Subscript in expression"
        },
        {
            "id": "subscript_020",
            "input": "{10, 20, 30}[1:3] * 2",
            "expected": [
                40,
                60
            ],
            "expectedType": "NodeVector",
            "notes": "Slice result in arithmetic"
        },
        {
            "id": "subscript_021",
            "input": "[1, 2; 3, 4][0]",
            "expected": [
                1,
                2
            ],
            "expectedType": "NodeVector",
            "notes": "Matrix single index returns row"
        },
        {
            "id": "subscript_022",
            "input": "(1..10)[2:5]",
            "expected": [
                3,
                4,
                5
            ],
            "expectedType": "NodeVector",
            "notes": "Slice a range (parentheses required for precedence)"
        }
    ]
}
